# 数据库配置
DB_HOST=your-mysql-host.rds.aliyuncs.com
DB_PORT=3306
DB_USERNAME=kitchenmate_user
DB_PASSWORD=your_secure_password
DB_DATABASE=kitchenmate

# JWT配置
JWT_SECRET=your_super_secret_jwt_key
JWT_EXPIRES_IN=7d

# Redis配置 (用于缓存)
REDIS_HOST=your-redis-host.redis.rds.aliyuncs.com
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# 应用配置
APP_PORT=7001
NODE_ENV=development

# 阿里云配置
ALIYUN_ACCESS_KEY_ID=your_access_key
ALIYUN_ACCESS_KEY_SECRET=your_secret_key
ALIYUN_REGION=cn-hangzhou